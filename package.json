{
  "name": "paneorama",
  "author": "manaten@manaten.net",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "dev": "vite",
    "fix": "run-s fix:*",
    "fix:js": "run-s \"lint:js -- --fix\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "gen:icons": "npx icon-gen -i ./public/assets/icon.svg -o ./public -r --ico --ico-name=favicon --ico-sizes=48 --favicon --favicon-name=assets/icon- --favicon-png-sizes=192,512",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "lint:js": "eslint **/*.{js,jsx,mjs,ts,tsx}",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "node src/index.ts",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@material-design-icons/svg": "^0.14.13",
    "classnames": "^2.5.1",
    "react": "^19",
    "react-dom": "^19",
    "react-rnd": "^10.4.12",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/builder-vite": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-better-tailwindcss": "^3.7.9",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "storybook": "^9.0.11",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^3.2.4"
  }
}
